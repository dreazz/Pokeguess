 console.log(this.player.lives);
  if (this.player.lives >= 1 && this.player.points < this.maxPoints) {
    if (this.pokemon.name === pokeForm.pokeName.value) {
      this.player.points += 1;
      //this.pokemon.hallOfFame.push({name:this.pokemon.name, sprite:this.pokemon.sprites.front_default})
      apiCall();
      this.printGameScreen();
      console.log("correct " + this.player.points);
    } else {
      if (this.player.lives === 1) {
        this.player.lives -= 1;
        this.printGameOverScreen();
      } else {
        this.player.lives -= 1;
        console.log("wrong as fuck " + this.player.lives);
        apiCall();
        this.printGameScreen();
      }
    }
  } else {
    this.printGameOverScreen();
  }